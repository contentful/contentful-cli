{
  "name": "contentful-cli",
  "version": "0.16.1",
  "description": "Contentful CLI tool",
  "main": "bin/contentful.js",
  "bin": {
    "contentful": "bin/contentful.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/contentful/contentful-cli.git"
  },
  "author": "Contentful <opensource@contentful.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && babel lib -d dist",
    "build:standalone": "npm run build && pkg --targets node8-macos-x64,node8-linux-x64,node8-win-x64 --debug --out-dir build .",
    "build:package": "npm run build:standalone && script/package",
    "clean": "rimraf dist",
    "dev": "nodemon --exec npm run build -w lib",
    "lint": "eslint bin lib test",
    "test": "npm run test:coverage",
    "test:coverage": "BABEL_ENV=test nyc npm run test:unit",
    "test:html-coverage": "BABEL_ENV=test nyc --reporter=html npm run test:unit",
    "test:unit": "BABEL_ENV=test jest test/unit/** --verbose",
    "test:unit:watch": "BABEL_ENV=test jest test/unit/** --verbose --watch",
    "integration:pre-test": "test/integration/scripts/pre-test.sh && npm run add-talkback-proxy",
    "integration:post-test": "test/integration/scripts/post-test.sh && npm run remove-talkback-proxy",
    "test:integration:recordings": "concurrently \"npm:run-talkback-proxy\" \"npm run integration:pre-test && npm run test:integration -- --runInBand && npm run integration:post-test\" --kill-others",
    "test:integration": "BABEL_ENV=test jest test/integration/cmds/**",
    "test:integration:watch": "npm run test:integration -- --watch",
    "test:integration:update": "npm run test:integration -- --updateSnapshot",
    "test:simulate-ci": "trevor",
    "precommit": "lint-staged",
    "prepush": "BABEL_ENV=test jest --changedSince master test/unit/**",
    "add-talkback-proxy": "./bin/contentful.js config add --proxy http://localhost:3333",
    "remove-talkback-proxy": "./bin/contentful.js config remove --proxy",
    "run-talkback-proxy": "node test/proxy.js false"
  },
  "devDependencies": {
    "app-root-path": "^2.1.0",
    "axios": "^0.19.0-beta.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.24.1",
    "concurrently": "^4.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "http-proxy": "^1.17.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.2",
    "nixt": "^0.5.0",
    "node-zip": "^1.1.1",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "pkg": "^4.3.3",
    "rimraf": "^2.6.1",
    "stream-buffers": "^3.0.2",
    "strip-ansi": "^4.0.0",
    "talkback": "^1.6.0",
    "trevor": "^2.3.0"
  },
  "dependencies": {
    "ast-types": "^0.11.5",
    "bfj": "^6.1.1",
    "blessed": "^0.1.81",
    "bluebird": "^3.5.0",
    "camelcase": "^5.0.0",
    "chalk": "^2.4.1",
    "cli-table3": "^0.5.1",
    "command-exists": "^1.2.7",
    "contentful-export": "^7.3.0",
    "contentful-import": "^7.3.0",
    "contentful-management": "^5.3.2",
    "contentful-migration": "^0.12.1",
    "emojic": "^1.1.11",
    "execa": "^0.10.0",
    "figlet": "^1.2.0",
    "find-up": "^3.0.0",
    "http-proxy-agent": "^2.1.0",
    "https-proxy-agent": "^2.2.1",
    "inquirer": "^6.0.0",
    "inquirer-select-directory": "^1.1.2",
    "listr": "^0.14.1",
    "lodash": "^4.17.10",
    "markdown-cli": "^1.6.0",
    "mkdirp": "^0.5.1",
    "mz": "^2.6.0",
    "opn": "^5.3.0",
    "path": "^0.12.7",
    "prettier": "^1.14.0",
    "recast": "^0.15.3",
    "rxjs": "^5.5.6",
    "shell-escape": "^0.2.0",
    "tar": "^4.4.4",
    "to-ast": "^1.0.0",
    "tree-kill": "^1.1.0",
    "wrap-ansi": "^3.0.1",
    "yargs": "^12.0.1",
    "zlib": "^1.0.5"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "bin",
    "dist",
    "docs",
    "version.js",
    "README.md"
  ],
  "jest": {
    "testURL": "http://localhost/"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "pkg": {
    "scripts": [
      "./node_modules/blessed/lib/widgets/*.js",
      "./dist/**/*.js"
    ],
    "assets": [
      "./node_modules/figlet/fonts/Standard.flf"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
